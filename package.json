{
  "name": "generator-nm5",
  "description": "A Cross-Platform Node.js ES5 Module Generator For OS X, Linux And Windows.",
  "version": "3.0.1",
  "author": {
    "name": "Sanemat",
    "email": "ogataken@gmail.com",
    "url": "http://sane.jp"
  },
  "dependencies": {
    "humanize-url": "^1.0.1",
    "normalize-url": "^1.3.0",
    "underscore.string": "^3.2.0",
    "yeoman-generator": "^0.20.3"
  },
  "devDependencies": {
    "conventional-changelog": "^0.4.2",
    "eclint": "^1.1.1",
    "eslint": "^1.2.1",
    "fixpack": "^2.2.0",
    "globstar": "^1.0.0",
    "mocha": "^2.2.5",
    "npm-check-updates": "^2.2.0",
    "run-yo": "^0.2.2",
    "touch": "^1.0.0",
    "yeoman-assert": "^2.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "app"
  ],
  "keywords": [
    "boilerplate",
    "module",
    "node",
    "node-module",
    "node_module",
    "plugin",
    "scaffold",
    "template",
    "yeoman-generator"
  ],
  "license": "MIT",
  "repository": "pandawing/generator-nm5",
  "scripts": {
    "changelog": "touch changelog.md && conventional-changelog -i changelog.md --overwrite --preset angular",
    "eclint": "globstar --ignore \"{{,**/}node_modules/**,temp/**}\" -- eclint check \"{,**/}*.{js,yml,md,json}\"",
    "eslint": "eslint .",
    "example": "run-yo",
    "fixpack": "fixpack",
    "git:tag": "git tag v${npm_package_version}",
    "lint": "npm run eclint && npm run eslint",
    "ncu": "ncu -ua",
    "test": "mocha",
    "verify": "npm run lint && npm run test"
  }
