{
  "name": "@sanemat/generator-nm",
  "description": "Scaffold out a node module",
  "version": "2.0.4",
  "author": {
    "name": "Sanemat",
    "email": "ogataken@gmail.com",
    "url": "http://sane.jp"
  },
  "dependencies": {
    "humanize-url": "^1.0.1",
    "normalize-url": "^1.2.0",
    "underscore.string": "^3.0.3",
    "yeoman-generator": "^0.20.1"
  },
  "devDependencies": {
    "conventional-changelog": "0.1.0-beta.1",
    "david": "^6.1.6",
    "eclint": "^0.2.6",
    "eslint": "^1.0.0-rc-1",
    "fixpack": "^2.2.0",
    "globstar": "^1.0.0",
    "lnfs-cli": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "*",
    "nodetree": "^0.0.2",
    "rimraf": "^2.4.1",
    "touch": "^1.0.0",
    "yeoman-assert": "^2.0.0",
    "yo": "^1.4.7"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "app"
  ],
  "keywords": [
    "boilerplate",
    "module",
    "node",
    "node-module",
    "node_module",
    "plugin",
    "scaffold",
    "template",
    "yeoman-generator"
  ],
  "license": "MIT",
  "repository": "pandawing/generator-nm",
  "scripts": {
    "changelog": "touch changelog.md && conventional-changelog -i changelog.md --overwrite --preset angular",
    "david": "david update",
    "eclint": "globstar --ignore \"{{,**/}node_modules/**,temp/**}\" -- eclint check \"{,**/}*.{js,yml,md,json}\"",
    "eslint": "eslint .",
    "example": "npm run example:link && npm run example:build && npm run example:unlink",
    "example:build": "cd example && ../node_modules/.bin/yo @sanemat/nm && cd ..",
    "example:link": "rimraf example && mkdirp example/node_modules/@sanemat && lnfs . example/node_modules/@sanemat/generator-nm",
    "example:unlink": "rimraf example/node_modules/@sanemat/generator-nm",
    "fixpack": "fixpack",
    "git:tag": "git tag v${npm_package_version}",
    "lint": "npm run eclint && npm run eslint",
    "test": "mocha",
    "verify": "npm run lint && npm run test && npm run example:link && npm run verify:link && npm run example:unlink && npm run verify:link",
    "verify:link": "nodetree -L 2 example/node_modules/@sanemat/"
  }
}
