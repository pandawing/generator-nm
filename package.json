    "underscore.string": "^3.2.0",
    "yeoman-generator": "^4.12.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.1.0",
    "eclint": "^2.8.1",
    "eslint": "^7.9.0",
    "fixpack": "^3.0.6",
    "globstar": "^1.0.0",
    "mocha": "^8.1.3",
    "npm-check-updates": "^9.0.1",
    "run-yo": "^1.0.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^3.0.0"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "files": [
    "app"
  ],
  "keywords": [
    "boilerplate",
    "module",
    "node",
    "node-module",
    "node_module",
    "plugin",
    "scaffold",
