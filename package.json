{
  "name": "generator-nm5",
  "description": "A Cross-Platform Node.js ES5 Module Generator For OS X, Linux And Windows.",
  "version": "3.3.0",
  "author": {
    "name": "Sanemat",
    "email": "ogataken@gmail.com",
    "url": "http://sane.jp"
  },
  "dependencies": {
    "humanize-url": "^2.1.0",
